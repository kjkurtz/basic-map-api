{
  "name": "basic-map-api",
  "version": "1.0.0",
  "description": "Backend for serving map data",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .js,.ts --fix",
    "start": "node dist/server.js",
    "serve-debug": "nodemon -r dotenv/config --inspect dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w"
  },
  "repository": "https://github.com/kjkurtz/basic-map-api",
  "dependencies": {
    "@mapbox/geojson-rewind": "^0.5.1",
    "@mapbox/mapbox-sdk": "^0.13.0",
    "@mapbox/sphericalmercator": "^1.1.0",
    "@turf/area": "^6.5.0",
    "@turf/bbox": "^6.5.0",
    "@turf/center": "^6.5.0",
    "@turf/helpers": "^6.1.4",
    "@types/express": "^4.17.13",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "hat": "^0.0.3",
    "moment": "^2.27.0",
    "pg": "^8.8.0",
    "simplify-geojson": "^1.0.4",
    "verror": "^1.10.0",
    "viewport-mercator-project": "^7.0.3",
    "winston": "^3.3.3",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/geojson": "^7946.0.8",
    "@types/mapbox__sphericalmercator": "^1.1.5",
    "@types/pg": "^8.6.5",
    "@types/pg-pool": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "babel-eslint": "^10.1.0",
    "concurrently": "^6.2.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}